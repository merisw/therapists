<%= form_for(@therapist) do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :therapist_type %>
    <%= f.text_field :therapist_type %>
  </p>

  <p>
    <%= f.label :fTE %>
    <%= f.text_field :fTE %>
  </p>
  <%= f.submit %>
<% end %>
